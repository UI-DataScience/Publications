	keywords = {Astrophysics - Astrophysics of Galaxies, Astrophysics - Cosmology and Nongalactic Astrophysics, Astrophysics - Instrumentation and Methods for Astrophysics, Physics - Data Analysis, Statistics and Probability},
	file = {arXiv\:1504.07255 PDF:C\:\\Users\\Sam\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\m2t1zc6b.default\\zotero\\storage\\7RGBSZZC\\Hoyle - 2015 - Measuring photometric redshifts using galaxy image.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\Sam\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\m2t1zc6b.default\\zotero\\storage\\DCXQXKI4\\1504.html:text/html}
